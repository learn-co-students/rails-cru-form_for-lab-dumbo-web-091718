<h1>Some Of Our Genres</h1>
<% @genres.each do |g| %>
<h3><%= link_to g.name, genre_path(g) %></h3><br>
<% end %>

<%= button_to "Add A New Genre", new_genre_path, {method: "get"}%><br>
<%= button_to "View All Songs", songs_path, {method: "get"} %><br>
<%= button_to "View All Artists", artists_path, {method: "get"} %><br>
