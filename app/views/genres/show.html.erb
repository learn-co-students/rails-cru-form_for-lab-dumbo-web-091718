<h1><%= @genre.name %></h1>
<h3> Artists In This Genre </h3><br>
<% @art_arr.each do |artist| %>
  <%= link_to artist.name, artist_path(artist) %><br>
<% end %>
<h3> Songs In This Genre </h3><br>
<% @genre.songs.each do |song| %>
  <%= link_to song.name, song_path(song) %><br>
<% end %><br>


<%= button_to "Edit This Genre", edit_genre_path, {method: "get"} %>
<%= button_to "Delete This Genre", @genre, {method: "delete"} %>
<%= button_to "View All Genres", genres_path, {method: "get"} %>
