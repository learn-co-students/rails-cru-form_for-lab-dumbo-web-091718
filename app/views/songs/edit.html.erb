<h1> Update Song </h1>
<%= form_for @song do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br></br>
<br></br>

<%= f.label :artist %>
<%= f.text_field :artist_id%>
<br></br>
<br></br>

<%= f.label :genre %>
<%= f.text_field :genre_id%>
<br></br>
<br></br>

<%= f.submit "Update Song" %>
<%end%>
